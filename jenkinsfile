pipeline{
agent any
stages{
   stage ('Build'){
    steps{
       echo "Declarative pipe line ---Building the project ....."

    }
   }
   stage ('Test'){
       steps{
          echo "Declarative pipe line ---Testing the project ....."
          sh "mvn clean test"

       }
      }
      stage ('Deploy'){
          steps{
             echo "Declarative pipe line ---Deploying the project ....."

          }
         }
         stage ('Release'){
             steps{
                echo "Releasing pipe line ---Building the project ....."
             }
            }
}
}